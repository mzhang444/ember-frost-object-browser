{{#frost-object-browser searchMode=demoSearchMode as |slot|}}
  {{#block-slot slot 'info-bar'}}
    {{#frost-info-bar as |slot|}}
      {{#block-slot slot 'title'}}
        Resources
      {{/block-slot}}
      {{#block-slot slot 'summary'}}
        20 â€“ L3VPN Services
      {{/block-slot}}
      {{#block-slot slot 'context-controls'}}
        {{frost-object-browser-search
            searchFilterModel=demoSearchFilterModel
            searchFilterValue=demoSearchFilterValue
            searchFilterView=demoSearchFilterView
            searchFilterRenderers=demoSearchFilterRenderers
            searchMode=demoSearchMode
            onSearchModeChange=(action 'onSearchModeChange')
            onSearchFilterChange=(action 'onSearchFilterChange')
        }}
      {{/block-slot}}
      {{#block-slot slot 'actions' as |controls|}}
        {{controls.button
          icon='infobar-create'
          onClick=(action 'onCreate')
          text='Create'
        }}
      {{/block-slot}}
    {{/frost-info-bar}}
  {{/block-slot}}
  {{#block-slot slot 'facets'}}
    {{!frost-bunsen...}}
    {{frost-object-browser-filter filters=filters onFilter=onOptionSelected}}
  {{/block-slot}}
  {{#block-slot slot 'view' as |onSelect selections|}}
   {{demo/content-view
    itemsPerPage=6
    model=model.model
    onDetailChange=(action "onDetailChange")
    onRowSelect=onSelect
    values=model.resources
    viewSchema=viewSchema
   }}
  {{/block-slot}}
  {{#block-slot slot 'actions' as |controls selections|}}
    {{controls.button
      onClick=(action 'delete')
      multiSelect=true
      priority="secondary"
      text='Delete'
    }}
    {{controls.button
      onClick=(action 'edit')
      priority="secondary"
      text='Edit'
    }}
    {{#controls.link 'details'
      multiSelect=true
      priority='primary'
    }}
      Details
    {{/controls.link}}
  {{/block-slot}}
{{/frost-object-browser}}
